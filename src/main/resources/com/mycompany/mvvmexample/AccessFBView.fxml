<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Menu?>
<?import javafx.scene.control.MenuBar?>
<?import javafx.scene.control.MenuItem?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<AnchorPane prefHeight="259.0" prefWidth="496.0" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="modelview.AccessFBView">
   <children>
      <VBox alignment="CENTER_RIGHT" layoutX="6.0" layoutY="52.0" prefHeight="166.0" prefWidth="485.0" spacing="10.0">
         <children>
            <TextField fx:id="nameField" alignment="TOP_LEFT" promptText="Name" />
            <TextField fx:id="majorField" promptText="Major" />
            <TextField fx:id="ageField" promptText="Age" />
            <HBox prefHeight="32.0" prefWidth="465.0">
               <children>
                  <Button fx:id="switchroot" mnemonicParsing="false" onAction="#switchToSecondary" text="Switch" textAlignment="CENTER" />
               </children>
            </HBox>
         </children>
         <padding>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </padding>
      </VBox>
      <TextArea fx:id="outputField" layoutX="5.0" layoutY="209.0" prefHeight="304.0" prefWidth="485.0" />
      <MenuBar layoutX="14.0" layoutY="14.0">
        <menus>
          <Menu mnemonicParsing="false" text="Profile">
            <items>
              <MenuItem mnemonicParsing="false" text="Close" />
            </items>
          </Menu>
          <Menu mnemonicParsing="false" text="Database">
            <items>
              <MenuItem mnemonicParsing="false" onAction="#addRecord" text="Write" />
                  <MenuItem mnemonicParsing="false" onAction="#readRecord" text="Read" />
            </items>
          </Menu>
          <Menu mnemonicParsing="false" text="Help">
            <items>
              <MenuItem mnemonicParsing="false" text="About" />
            </items>
          </Menu>
        </menus>
      </MenuBar>
   </children>
   <opaqueInsets>
      <Insets />
   </opaqueInsets>
</AnchorPane>
