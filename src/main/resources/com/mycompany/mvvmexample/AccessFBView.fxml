<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Menu?>
<?import javafx.scene.control.MenuBar?>
<?import javafx.scene.control.MenuItem?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane minHeight="300.0" minWidth="450.0" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="modelview.AccessFBView">
   <children>
      <VBox alignment="CENTER_RIGHT" layoutX="6.0" layoutY="52.0" prefWidth="485.0" spacing="10.0" AnchorPane.topAnchor="100.0">
         <children>
            <Label fx:id="welcomeMessageLabel" alignment="CENTER" prefWidth="500.0" text="Welcome Back">
               <font>
                  <Font name="Gill Sans MT" size="24.0" />
               </font>
            </Label>
            <HBox alignment="CENTER" prefHeight="32.0" prefWidth="465.0">
               <children>
                  <Button fx:id="switchroot" alignment="CENTER" mnemonicParsing="false" onAction="#switchToSecondary" text="Switch" textAlignment="CENTER" />
               </children>
               <VBox.margin>
                  <Insets top="15.0" />
               </VBox.margin>
            </HBox>
         </children>
         <padding>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </padding>
      </VBox>
      <MenuBar layoutX="14.0" layoutY="14.0">
        <menus>
          <Menu mnemonicParsing="false" text="Database">
            <items>
              <MenuItem mnemonicParsing="false" onAction="#displayDatabaseMenuPress" text="Display" />
            </items>
          </Menu>
            <Menu mnemonicParsing="false" text="Options">
               <items>
                  <MenuItem mnemonicParsing="false" onAction="#logOutMenuPress" text="Log out" />
               </items>
            </Menu>
        </menus>
      </MenuBar>
   </children>
   <opaqueInsets>
      <Insets />
   </opaqueInsets>
</AnchorPane>
